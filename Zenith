local vim = game:GetService'VirtualInputManager'

pcall(function()
    local b = plr.PlayerGui.DeathMenu.Frame.Button.TextButton
    b.Parent = plr.PlayerGui.DeathMenu
    b.Size = UDim2.new(0.1,0,0.05,0)
    b.Position = UDim2.new(0.1,0,0,0)
    b.TextScaled = true
end)

plr.CharacterAdded:Connect(function()
    repeat rs.RenderStepped:wait() until plr:FindFirstChild'PlayerGui' and plr.PlayerGui:FindFirstChild'DeathMenu' and plr.PlayerGui.DeathMenu:FindFirstChild'Frame' and plr.PlayerGui.DeathMenu.Frame:FindFirstChild'Button' and plr.PlayerGui.DeathMenu.Frame.Button:FindFirstChild'TextButton'
    local b = plr.PlayerGui.DeathMenu.Frame.Button.TextButton
    b.Parent = plr.PlayerGui.DeathMenu
    b.Size = UDim2.new(0.1,0,0.05,0)
    b.Position = UDim2.new(0.1,0,0,0)
    b.TextScaled = true
end)

plr.CharacterAdded:Connect(function()
    local c = 0
    repeat 
        rs.RenderStepped:wait()
        c=c+1
    until plr.Character and plr.Character:FindFirstChild'HumanoidRootPart' and plr.Character:FindFirstChild'RightHand' and plr:FindFirstChild'Backpack' and plr.Backpack:FindFirstChild'Punch' and plr:FindFirstChild'PlayerGui' and plr.PlayerGui:FindFirstChild'DeathMenu' and plr.PlayerGui.DeathMenu:FindFirstChild'TextButton' or c>=200
    local obj = plr.PlayerGui.DeathMenu.TextButton
    if c>=200 then
        vim:SendMouseButtonEvent(obj.AbsolutePosition.X+50,obj.AbsolutePosition.Y+50,0,true,game,0)
        vim:SendMouseButtonEvent(obj.AbsolutePosition.X+50,obj.AbsolutePosition.Y+50,0,false,game,0)
    else
        pcall(function()
            plr.Backpack.Punch.Parent = plr.Character
            plr.Character.Punch:Activate()
            firetouchinterest(plr.Character.RightHand,plr.Character.HumanoidRootPart,0)
            firetouchinterest(plr.Character.RightHand,plr.Character.HumanoidRootPart,1)
            wait()
            vim:SendMouseButtonEvent(obj.AbsolutePosition.X+50,obj.AbsolutePosition.Y+50,0,true,game,0)
            vim:SendMouseButtonEvent(obj.AbsolutePosition.X+50,obj.AbsolutePosition.Y+50,0,false,game,0)
        end)
    end
end)
  	end    
})

Tab5:AddButton({
	Name = "AutoFarm",
	Callback = function()
        local user = tostring(game.Players.LocalPlayer)
    local OnOff = true
    while OnOff == true do
        pcall(function()
            local points = game:GetService("Players").LocalPlayer.leaderstats["Bully Points"].Value
            if game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Punch") then
                game:GetService("Players").LocalPlayer.Backpack.Punch.Parent = game:GetService("Workspace"):FindFirstChild(user)
            end
            wait()
            local bb=game:service'VirtualUser'
            bb:CaptureController()bb:ClickButton1(Vector2.new())
            wait()
            while not game:GetService("Workspace"):FindFirstChild(user):FindFirstChild("Humanoid") do
                wait()
            end
            game:GetService("Players").LocalPlayer.PlayerGui.DeathMenu.Frame.Visible = true
            game:GetService("Workspace"):FindFirstChild(user).Humanoid.Health = -100
            game:GetService("Players").LocalPlayer.PlayerGui.DeathMenu.Frame.Button.Position = UDim2.new(-1, 0, 0, 0)
            game:GetService("Players").LocalPlayer.PlayerGui.DeathMenu.Frame.Button.TextButton.Size = UDim2.new(100, 100, 100, 100)
            wait(0.1)
            bb:CaptureController()bb:ClickButton1(Vector2.new())
            wait(0.1)
            if game:GetService("Players").LocalPlayer.PlayerGui.DeathMenu.Frame.Visible == true then
                wait(0.3)
                bb:CaptureController()bb:ClickButton1(Vector2.new())
            end
            while not game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Punch") do
                wait()
            end
        end)
    end
end    
})



