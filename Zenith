	Name = "Hidden Loop",
	Default = false,
	Callback = function(Value)
		hiddenloop = Value
	end    
})
spawn(function()
    while true do
        pcall(function()
            player.Character.ArmAngle.Disabled = hiddenloop
            wait(0.3)
        end)
    end
end)


